    "mocha": "^8.1.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "sinon": "^9.2.4",
    "supertest": "^6.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.3.5"
  }
