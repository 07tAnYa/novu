  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker:build": "pnpm --silent --workspace-root pnpm-context -- apps/inbound-mail/Dockerfile | docker build --build-arg PACKAGE_PATH=apps/inbound-mail - -t novu-inbound-mail",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon",
    "start:dev": "nodemon",
    "start:test": "cross-env NODE_ENV=test TZ=UTC nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"strictNullChecks\": false}' TZ=UTC NODE_ENV=test E2E_RUNNER=true mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts src/**/**/*.spec.ts",
    "test:e2e": "cross-env TS_NODE_COMPILER_OPTIONS='{\"strictNullChecks\": false}' TZ=UTC NODE_ENV=test E2E_RUNNER=true mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts e2e/**/*.e2e.ts src/**/*.e2e.ts"
  },
  "dependencies": {
    "@novu/application-generic": "^0.10.2",
    "@novu/dal": "^0.10.2",
    "@novu/shared": "^0.10.2",
    "@novu/stateless": "^0.10.2",
    "@novu/testing": "^0.10.2",
    "@sentry/node": "^6.3.3",
    "node-uuid": "^1.4.8",
    "axios": "^0.24.0",
    "bull": "^4.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "extend": "^3.0.2",
    "html-to-text": "^9.0.3",
    "jsonwebtoken": "9.0.0",
    "languagedetect": "^2.0.0",
    "lodash": "^4.17.15",
    "nest-raven": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "smtp-server": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^8.0.1",
